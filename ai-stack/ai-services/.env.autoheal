IMAGE="willfarrell/autoheal"
NAME="autoheal-management"
RESTART="unless-stopped"

HOST_UID="1000"
HOST_GID="1000"

AUTOHEAL_CONTAINER_LABEL=keep_healthy
AUTOHEAL_START_PERIOD=0
AUTOHEAL_INTERVAL=5
AUTOHEAL_DEFAULT_STOP_TIMEOUT=10
AUTOHEAL_ONLY_MONITOR_RUNNING=false