GF_FEATURE_TOGGLES_ENABLE=newNavigation
GF_INSTALL_IMAGE_RENDERER_PLUGIN=true
GF_LOG_LEVEL=debug
GF_LOG_MODE="console file"
GF_PLUGIN_GRAFANA_IMAGE_RENDERER_RENDERING_IGNORE_HTTPS_ERRORS=true
GF_SECURITY_ADMIN_PASSWORD__FILE=/run/secrets/grafana_password
GF_SECURITY_ADMIN_USER=/run/secrets/grafana_username
# GF_PLUGINS_PREINSTALL="grafana-clock-panel, grafana-simple-json-datasource"
# GF_SERVER_ROOT_URL=http://my.grafana.server/
IMAGE=grafana/grafana:latest
NAME=grafana
PGID=1000
PORT=3000
PUID=1000
RESTART=unless-stopped
