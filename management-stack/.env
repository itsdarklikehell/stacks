# =====================
# GLOBAL/COMMON
# =====================
STACK_VERSION=1.0.0
HOST_UID=1000
HOST_GID=1000
TZ=Europe/Amsterdam

OPENAI_API_KEY=
GROQ_API_KEY=
ANTHROPIC_API_KEY=
OLLAMA_BASE_URL=http://localhost:11434
AZURE_API_KEY=
AZURE_BASE_URL=
AZURE_API_VERSION=
GEMINI_API_KEY=
VLLM_API_BASE=
OPENLLM_AUTH_TYPE=
OPENLLM_API_KEY=
LETTA_OTEL_EXPORTER_OTLP_ENDPOINT=
CLICKHOUSE_ENDPOINT=http://clickhouse-server:9000
CLICKHOUSE_DATABASE=
CLICKHOUSE_USERNAME=
CLICKHOUSE_PASSWORD=
CLICKHOUSE_SERVER_IMAGE=clickhouse/clickhouse-server

# =====================
# AUTOHEAL
# =====================
AUTOHEAL_IMAGE=willfarrell/autoheal:latest
autoheal_container_name=autoheal-management
autoheal_environment_AUTOHEAL_CONTAINER_LABEL=keep_healthy
autoheal_container_network_mode=none
autoheal_restart=always

# =====================
# UPTIME KUMA
# =====================
UPTIME_KUMA_IMAGE=louislam/uptime-kuma:latest
uptime_kuma_container_name=uptime-kuma
uptime_kuma_restart=always
UPTIME_KUMA_PORT=3002
UPTIME_KUMA_UMASK=0022

# =====================
# NETDATA
# =====================
NETDATA_IMAGE=netdata/netdata:edge
netdata_container_name=netdata
netdata_restart=unless-stopped

# =====================
# N8N
# =====================
N8N_IMAGE=docker.n8n.io/n8nio/n8n
n8n_container_name=n8n
n8n_restart=always
SUBDOMAIN=n8n
DOMAIN_NAME=example.com
GENERIC_TIMEZONE=${TZ}

# =====================
# DASHY
# =====================
DASHY_IMAGE=lissy93/dashy
dashy_container_name=Dashy
dashy_restart=unless-stopped

# =====================
# DOCKGE
# =====================
DOCKGE_IMAGE=louislam/dockge:1
dockge_container_name=dockge
dockge_hostname=dockge
dockge_restart=unless-stopped

# =====================
# PORTAINER
# =====================
PORTAINER_IMAGE=portainer/portainer-ce:latest
portainer_container_name=portainer
portainer_hostname=portainer
portainer_restart=unless-stopped

# =====================
# NGINX PROXY MANAGER
# =====================
NGINX_PROXY_MANAGER_IMAGE=jc21/nginx-proxy-manager:latest
nginx_proxy_manager_container_name=nginx-proxy-manager
nginx_proxy_manager_hostname=nginx-proxy-manager
nginx_proxy_manager_restart=unless-stopped
# NGINX_PROXY_MANAGER_USERNAME=admin@example.com
# NGINX_PROXY_MANAGER_PASSWORD=changeme

# =====================
# GRAFANA
# =====================
GRAFANA_IMAGE=grafana/grafana
grafana_container_name=grafana
grafana_restart=unless-stopped
grafana_admin=admin
grafana_password=admin

# =====================
# DOCKER PROXY
# =====================
DOCKER_PROXY_IMAGE=tecnativa/docker-socket-proxy:latest
